<?xml version='1.0' encoding='utf-8'?>
<widget id="com.MiniProject.G18" version="1.0.0">
<icon src ="res\icon.png" platform="android" density="hdpi"/>
<name>Object Locator</name>
<icon src="res/icon.png" platform="android" width="57" height="57" density="mdpi" />
    <name>Object Locator</name>
    <description>
        An object Locator exclusive app built using Apache Cordova
    </description>
    <author>
        MiniProject Group18
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
